{
  "private": true,
  "name": "freshdesk-react-typescript-starter",
  "version": "1.0.0",
  "author": "Drew Gauderman <drew@dpg.host> (https://dpg.host/)",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-client": "webpack --mode development --watch",
    "dev-server": "tsc -w -p ./tsconfig.server.json",
    "build-client": "webpack --mode production",
    "build-server": "tsc --p ./tsconfig.server.json",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "fdk": "fdk run",
    "pack": "fdk pack --skip-validation lint"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@types/jquery": "^3.3.38",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "concurrently": "^5.2.0",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3"
  }
}